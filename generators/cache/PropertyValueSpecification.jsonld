        "@id": "schema:PropertyValueSpecification"
      },
      "schema:rangeIncludes": [
        {
          "@id": "schema:Text"
        },
        {
          "@id": "schema:Thing"
        }
      ]
    },
    {
      "@id": "schema:Intangible",
      "@type": "rdfs:Class",
