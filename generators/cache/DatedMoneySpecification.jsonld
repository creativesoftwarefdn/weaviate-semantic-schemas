      "rdfs:comment": "The amount of money.",
      "rdfs:label": "amount",
      "schema:category": "issue-1253",
      "schema:domainIncludes": [
        {
          "@id": "schema:DatedMoneySpecification"
        },
        {
          "@id": "schema:MoneyTransfer"
        },
        {
          "@id": "schema:LoanOrCredit"
        },
        {
          "@id": "schema:InvestmentOrDeposit"
        }
