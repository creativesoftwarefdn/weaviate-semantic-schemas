      "schema:domainIncludes": {
        "@id": "schema:MedicalEntity"
      },
      "schema:isPartOf": {
        "@id": "http://health-lifesci.schema.org"
      },
      "schema:rangeIncludes": {
        "@id": "schema:Organization"
      }
    },
    {
      "@id": "schema:study",
      "@type": "rdf:Property",
