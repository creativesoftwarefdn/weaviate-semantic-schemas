        {
          "@id": "schema:Specialty"
        },
        {
          "@id": "schema:MedicalEnumeration"
        }
      ],
      "schema:isPartOf": {
        "@id": "http://health-lifesci.schema.org"
      }
    },
    {
      "@id": "schema:drug",
