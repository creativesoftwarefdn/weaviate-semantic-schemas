      "schema:domainIncludes": {
        "@id": "schema:Thing"
      },
      "schema:rangeIncludes": {
        "@id": "schema:Text"
      }
    },
    {
      "@id": "schema:contraindication",
      "@type": "rdf:Property",
      "rdfs:comment": "A contraindication for this therapy.",
      "rdfs:label": "contraindication",
